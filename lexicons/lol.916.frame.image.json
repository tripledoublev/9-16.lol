{
	"lexicon": 1,
	"id": "lol.916.frame.image",
	"defs": {
		"main": {
			"type": "record",
			"key": "tid",
			"record": {
				"type": "object",
				"required": ["createdAt", "embed"],
				"properties": {
					"createdAt": {
						"type": "string",
						"format": "datetime"
					},
					"text": {
						"type": "string",
						"maxLength": 240
					},
					"embed": {
						"$ref": "app.bsky.embed.images"
					},
					"aspect": {
						"type": "string",
						"enum": ["9:16", "original", "other"]
					},
					"expiresAt": {
						"type": "string",
						"format": "datetime"
					}
				}
			}
		}
	}
}