{
	"lexicon": 1,
	"id": "lol.916.user.profile",
	"defs": {
		"main": {
			"type": "record",
			"key": "literal:self",
			"record": {
				"type": "object",
				"required": ["createdAt"],
				"properties": {
					"displayName": {
						"type": "string",
						"maxGraphemes": 64,
						"maxLength": 640
					},
					"bio": {
						"type": "string",
						"maxGraphemes": 256,
						"maxLength": 2560
					},
					"avatar": {
						"type": "blob",
						"accept": ["image/jpeg", "image/png", "image/webp"],
						"maxSize": 1000000
					},
					"createdAt": {
						"type": "string",
						"format": "datetime"
					}
				}
			}
		}
	}
}
